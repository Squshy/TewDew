{
    "name": "TewDew",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "eslint": "^8.41.0",
        "eslint-config-custom": "*",
        "prettier": "latest",
        "turbo": "latest"
    },
    "packageManager": "npm@8.1.2"
}
